{
  "name": "nba-scorebox",
  "version": "1.0.0",
  "description": "A minimal Electron application to watch nba and play by play",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build_mac": "electron-packager ./ nba-scorebox -out ./nba-scorebox-mac --platform=darwin --arch=x64 --electron-version 7.1.4 --icon=./app/img/icon/icon.ico --asar --ignore=node_modules/electron-* ",
    "build_win32": "electron-packager ./ nba-scorebox -out ./nba-scorebox --platform=win32 --arch=x64 --electron-version 7.1.4 --icon=./app/img/icon/icon.ico --asar --ignore=node_modules/electron-* ",
    "build_linux": "electron-packager ./ nba-scorebox -out ./nba-scorebox-linux --platform=linux --arch=x64 --electron-version 7.1.4 --icon=./app/img/icon/icon.ico --asar --ignore=node_modules/electron-* ",
    "packager":"electron-packager ./ --platform=all --arch=all --electron-vision=7.1.4 -out ./nba-scorebox --overwrite=true"
  },
  "repository": "https://github.com/ZJKung/zj-electron-nbascore",
  "keywords": [
    "Electron",
    "demo"
  ],
  "author": "ZJ",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^7.1.4",
    "electron-packager": "^14.1.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.4.1",
    "datatables.net-dt": "^1.10.20",
    "jquery": "^3.4.1",
    "moment": "^2.24.0"
  }
}
